    <rule id="06f7-edc9-b3f7-cb87" name="Mount&apos;s Protection (5+)" book="9th Age 0.99" page="77" hidden="false">
      <profiles/>
      <rules/>
      <infoLinks/>
      <modifiers/>
      <description>No matter how many mounts a model has, it still only gains the largest bonus once.</description>
    </rule>
    <rule id="2aa3-ad01-4626-4a79" name="Mount&apos;s Protection (6+)" book="9th Age 0.99" page="77" hidden="false">
