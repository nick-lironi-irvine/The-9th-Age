      <rules/>
      <infoLinks/>
      <modifiers/>
      <description>Wizard Apprentices add +1 to their casting and dispel rolls.</description>
    </rule>
    <rule id="32fd-b66c-a925-0b7d" name="Wizard Master" hidden="false">
      <profiles/>
      <rules/>
      <infoLinks/>
      <modifiers/>
      <description>Wizard Masters add +2 to their casting and dispel rolls.</description>
    </rule>
    <rule id="1cf9-6f2a-f79a-0dfa" name="Monstrous Ranks" hidden="false">
      <profiles/>
      <rules/>
      <infoLinks/>
      <modifiers/>
      <description>A unit with this rule only needs to be 1 model wide to form a Full Rank.</description>
    </rule>
